#: ../src/nautilus-annotations.c:129 ../src/nautilus-annotations.c:431
msgid "Annotations"
msgstr "Annotazioni"

#: ../src/nautilus-annotations.c:132
msgid "_OK"
msgstr "_OK"

#: ../src/nautilus-annotations.c:134
msgid "_Cancel"
msgstr "_Annulla"

#: ../src/nautilus-annotations.c:211
msgid "_Discard changes"
msgstr "_Scarta le modifiche"

#: ../src/nautilus-annotations.c:260
msgid "Could not erase file's annotations"
msgstr "Impossibile eliminare le annotazioni dal file"

#: ../src/nautilus-annotations.c:351
msgid "Could not save file's annotations"
msgstr "Impossibile salvare le annotazioni per il file"

#: ../src/nautilus-annotations.c:373
msgid "Are you sure you want to discard the current changes?"
msgstr "Si è sicuri di voler scartare le modifiche correnti?"

#: ../src/nautilus-annotations.c:437
msgid "Annotations shared between multiple files"
msgstr "Annotazioni condivise tra più file"

#: ../src/nautilus-annotations.c:557
msgid "No files were selected to be unannotated"
msgstr "Non ci sono file selezionati da cui rimuovere annotazioni"

#: ../src/nautilus-annotations.c:575 ../src/nautilus-annotations.c:642
msgid "Error allocating memory"
msgstr "Errore nell'allocare memoria"

#: ../src/nautilus-annotations.c:586
msgid "Do you really want to remove the annotations from the selected file?"
msgid_plural ""
"Do you really want to remove all annotations from the selected files?"
msgstr[0] "Si vogliono davvero eliminare le annotazioni presenti nel file?"
msgstr[1] ""
"Si vogliono davvero eliminare tutte le annotazioni presenti nei file "
"selezionati?"

#: ../src/nautilus-annotations.c:614
msgid "No files were selected to be annotated"
msgstr "Non ci sono file selezionati da annotare"

#: ../src/nautilus-annotations.c:663
msgid "Could not access file's annotations"
msgstr "Impossibile accedere alle annotazioni del file"

#: ../src/nautilus-annotations.c:698
msgid ""
"At least two annotations in the file selection differ.\n"
"This will set up a blank new note."
msgstr ""
"Almeno due annotazioni nella selezione differiscono.\n"
"Questo comporterà l'apertura di una annotazione vuota."

#: ../src/nautilus-annotations.c:782
msgid "_Remove annotations from file"
msgid_plural "_Remove annotations from files"
msgstr[0] "_Rimuovi le annotazioni dal file"
msgstr[1] "_Rimuovi le annotazioni dai file"

#: ../src/nautilus-annotations.c:788
msgid "Remove annotations from the selected file"
msgid_plural "Remove annotations from the selected files"
msgstr[0] "Rimuovi le annotazioni presenti nel file selezionato"
msgstr[1] "Rimuovi le annotazioni presenti nei file selezionati"

#: ../src/nautilus-annotations.c:825
msgid "Edit file's an_notations"
msgid_plural "Edit files' an_notations"
msgstr[0] "Modifica le an_notazioni del file"
msgstr[1] "Modifica le an_notazioni dei file"

#: ../src/nautilus-annotations.c:831
msgid "Edit the current annotations for this file"
msgid_plural "Edit the current annotations for these files"
msgstr[0] "Modifica le annotazioni correnti per questo file"
msgstr[1] "Modifica le annotazioni correnti per questi file"

#: ../src/nautilus-annotations.c:844
msgid "An_notate file"
msgid_plural "An_notate files"
msgstr[0] "An_nota il file"
msgstr[1] "An_nota i file"

#: ../src/nautilus-annotations.c:850
msgid "Attach a note to this file"
msgid_plural "Attach a note to these files"
msgstr[0] "Allega una nota a questo file"
msgstr[1] "Allega una nota a questi file"
