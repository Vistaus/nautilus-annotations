msgstr[1] "Modifica le annotazioni allegate agli oggetti selezionati"

#: ../src/nautilus-annotations.c:918
msgid "Remove the annotations attached to the selected object"
msgid_plural "Remove the annotations attached to the selected objects"
msgstr[0] "Rimuovi le annotazioni allegate all'oggetto selezionato"
msgstr[1] "Rimuovi le annotazioni allegate agli oggetti selezionati"

#: ../src/nautilus-annotations.c:948
msgid "_Annotate directory"
msgid_plural "_Annotate directories"
msgstr[0] "_Annota la cartella"
msgstr[1] "_Annota le cartelle"

#: ../src/nautilus-annotations.c:955
msgid "_Annotate file"
msgid_plural "_Annotate files"
msgstr[0] "_Annota il file"
msgstr[1] "_Annota i file"

#: ../src/nautilus-annotations.c:960
msgid "_Annotate objects"
msgstr "_Annota gli oggetti"

#: ../src/nautilus-annotations.c:963
msgid "Attach an annotation to the selected object"
msgid_plural "Attach an annotation to the selected objects"
msgstr[0] "Allega un'annotazione all'oggetto selezionato"
msgstr[1] "Allega un'annotazione agli oggetti selezionati"
