msgid "This will set up a blank new annotation."
msgstr "Questo comporterà l'apertura di un'annotazione vuota."

#: ../src/nautilus-annotations.c:728
msgid "No files were selected to be unannotated"
msgstr "Non ci sono file selezionati da cui rimuovere annotazioni"

#: ../src/nautilus-annotations.c:738
msgid "Do you really want to erase the annotations attached?"
msgstr "Si vogliono davvero rimuovere le annotazioni allegate?"

#: ../src/nautilus-annotations.c:739
msgid "The annotations will be lost forever."
msgstr "Le annotazioni andranno perse per sempre."

#: ../src/nautilus-annotations.c:740 ../src/nautilus-annotations.c:892
msgid "E_rase"
msgstr "_Rimuovi"

#: ../src/nautilus-annotations.c:843
msgid "Directory's _annotations"
msgid_plural "Directories' _annotations"
msgstr[0] "_Annotazioni della cartella"
msgstr[1] "_Annotazioni delle cartelle"

#: ../src/nautilus-annotations.c:850
msgid "File's _annotations"
msgid_plural "Files' _annotations"
msgstr[0] "_Annotazioni del file"
msgstr[1] "_Annotazioni dei file"

#: ../src/nautilus-annotations.c:855
msgid "Objects' _annotations"
msgstr "_Annotazioni degli oggetti"

#: ../src/nautilus-annotations.c:858
msgid "Choose an action for the object's annotations"
msgid_plural "Choose an action for the objects' annotations"
msgstr[0] "Scegli un'azione da compiere con le annotazioni dell'oggetto"
msgstr[1] "Scegli un'azione da compiere con le annotazioni degli oggetti"

#: ../src/nautilus-annotations.c:870
msgid "_Edit and extend"
msgstr "_Modifica ed estendi"

#: ../src/nautilus-annotations.c:872
msgid "_Edit"
msgstr "_Modifica"

#: ../src/nautilus-annotations.c:875
msgid "Edit and extend the annotations attached to the selected objects"
msgstr "Modifica ed estendi le annotazioni allegate agli oggetti selezionati"

#: ../src/nautilus-annotations.c:881
msgid "Edit the annotations attached to the selected object"
