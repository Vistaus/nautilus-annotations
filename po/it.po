msgid "Could not save file's annotations"
msgstr "Impossibile salvare le annotazioni per il file"

#: ../src/nautilus-annotations.c:328
msgid "Are you sure you want to discard the current changes?"
msgstr "Si è sicuri di voler scartare le modifiche correnti?"

#: ../src/nautilus-annotations.c:329
msgid "This action cannot be undone."
msgstr "Questa azione non potrà essere annullata."

#: ../src/nautilus-annotations.c:330 ../src/nautilus-annotations.c:377
msgid "_Discard changes"
msgstr "_Scarta le modifiche"

#: ../src/nautilus-annotations.c:419
msgid "Error allocating memory"
msgstr "Errore nell'allocare memoria"

#: ../src/nautilus-annotations.c:494
msgid "Annotations"
msgstr "Annotazioni"

#: ../src/nautilus-annotations.c:497
msgid "Unknown path"
msgstr "Percorso sconosciuto"

#: ../src/nautilus-annotations.c:504
msgid "Shared annotations"
msgstr "Annotazioni condivise"

#: ../src/nautilus-annotations.c:505
msgid "(multiple objects)"
msgstr "(più oggetti)"

