#: ../src/nautilus-annotations.c:225 ../src/nautilus-annotations.c:396
msgid "_Discard changes"
msgstr "_Scarta le modifiche"

#: ../src/nautilus-annotations.c:281
msgid "Could not erase file's annotations"
msgstr "Impossibile eliminare le annotazioni dal file"

#: ../src/nautilus-annotations.c:372
msgid "Could not save file's annotations"
msgstr "Impossibile salvare le annotazioni per il file"

#: ../src/nautilus-annotations.c:394
msgid "Are you sure you want to discard the current changes?"
msgstr "Si è sicuri di voler scartare le modifiche correnti?"

#: ../src/nautilus-annotations.c:395
msgid "This action cannot be undone."
msgstr "Questa operazione non potrà essere annullata."

#: ../src/nautilus-annotations.c:458
msgid "Annotations"
msgstr "Annotazioni"

#: ../src/nautilus-annotations.c:464
msgid "Annotations shared between multiple files"
msgstr "Annotazioni condivise tra più file"

#: ../src/nautilus-annotations.c:603
msgid "No files were selected to be unannotated"
msgstr "Non ci sono file selezionati da cui rimuovere annotazioni"

#: ../src/nautilus-annotations.c:621 ../src/nautilus-annotations.c:683
msgid "Error allocating memory"
msgstr "Errore nell'allocare memoria"

#: ../src/nautilus-annotations.c:629
msgid "Do you really want to erase the annotations attached?"
msgstr "Si vogliono davvero rimuovere le annotazioni allegate?"

#: ../src/nautilus-annotations.c:630
msgid "The annotations will be lost forever."
msgstr "Le annotazioni andranno perse per sempre."

#: ../src/nautilus-annotations.c:631 ../src/nautilus-annotations.c:915
msgid "E_rase"
msgstr "_Rimuovi"

#: ../src/nautilus-annotations.c:655
msgid "No files were selected to be annotated"
msgstr "Non ci sono file selezionati da annotare"

#: ../src/nautilus-annotations.c:704
msgid "Could not access file's annotations"
msgstr "Impossibile accedere alle annotazioni del file"

#: ../src/nautilus-annotations.c:737
msgid "At least two annotations in the file selection differ"
msgstr "Almeno due annotazioni nella selezione differiscono"

#: ../src/nautilus-annotations.c:738
msgid "This will set up a blank new annotation."
msgstr "Questo comporterà l'apertura di un'annotazione vuota."

#: ../src/nautilus-annotations.c:739
msgid "_OK"
msgstr "_OK"

#: ../src/nautilus-annotations.c:866
msgid "Directory's _annotations"
msgid_plural "Directories' _annotations"
msgstr[0] "_Annotazioni della cartella"
msgstr[1] "_Annotazioni delle cartelle"

#: ../src/nautilus-annotations.c:873
msgid "File's _annotations"
msgid_plural "Files' _annotations"
msgstr[0] "_Annotazioni del file"
msgstr[1] "_Annotazioni dei file"

#: ../src/nautilus-annotations.c:878
msgid "Objects' _annotations"
msgstr "_Annotazioni degli oggetti"

#: ../src/nautilus-annotations.c:881
msgid "Choose an action for the object's annotations"
msgid_plural "Choose an action for the objects' annotations"
msgstr[0] "Scegli un'azione da compiere con le annotazioni dell'oggetto"
msgstr[1] "Scegli un'azione da compiere con le annotazioni degli oggetti"

#: ../src/nautilus-annotations.c:893
msgid "_Edit and extend"
msgstr "_Modifica ed estendi"

#: ../src/nautilus-annotations.c:895
msgid "_Edit"
msgstr "_Modifica"

#: ../src/nautilus-annotations.c:898
msgid "Edit and extend the annotations attached to the selected objects"
msgstr "Modifica ed estendi le annotazioni allegate agli oggetti selezionati"

#: ../src/nautilus-annotations.c:904
msgid "Edit the annotations attached to the selected object"
msgid_plural "Edit the annotations attached to the selected objects"
