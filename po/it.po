msgstr[0] "_Annota la cartella"
msgstr[1] "_Annota le cartelle"

#: ../src/nautilus-annotations.c:932
msgid "_Annotate file"
msgid_plural "_Annotate files"
msgstr[0] "_Annota il file"
msgstr[1] "_Annota i file"

#: ../src/nautilus-annotations.c:937
msgid "_Annotate objects"
msgstr "_Annota gli oggetti"

#: ../src/nautilus-annotations.c:940
msgid "Attach an annotation to the selected object"
msgid_plural "Attach an annotation to the selected objects"
msgstr[0] "Allega un'annotazione all'oggetto selezionato"
msgstr[1] "Allega un'annotazione agli oggetti selezionati"
